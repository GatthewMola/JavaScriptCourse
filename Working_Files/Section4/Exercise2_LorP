// Write the statement for the isLandscape function.

//My Method
function isLandscape(width, height) {
    if (width > height) return true;
    return false;
}

console.log(isLandscape(2, 8));

// Can also use operational operator
function isLandscape(width, height) {
    return (width > height) ? true : false;
}

console.log(isLandscape(2, 8));

// Even cleaner!
function isLandscape(width, height) {
    return (width > height);
}

console.log(isLandscape(2, 8));